<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ff999999" android:endColor="#ff999999" android:angle="270.0" android:centerY="0.45" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ffff920a" android:endColor="#ffff920a" android:angle="270.0" android:centerY="0.45" />
            </shape>
        </clip>
    </item>
</layer-list>